  "cumleler": [
    {"ar": "أَنَا طَالِبٌ فِي الْمَدْرَسَة", "tr": "Ben okulda bir öğrenciyim.", "seviye": 1},
    {"ar": "هَلْ تَسْكُنُ هُنَا؟", "tr": "Burada mı oturuyorsun?", "sevi": 1},
    {"ar": "أُرِيدُ قَهْوَة سَاخِنَة", "tr": "Sıcak bir kahve istiyorum.", "sevi": 1},
    {"ar": "مَاذَا تَفْعَلُ الْآن؟", "tr": "Şimdi ne yapıyorsun?", "sevi": 1},
    {"ar": "كَيْفَ حَالُكَ الْيَوْم؟", "tr": "Bugün nasılsın?", "sevi": 1},
    {"ar": "السَّيَّارَةُ جَدِيدَةٌ وَكَبِيرَة", "tr": "Araba yeni ve büyüktür.", "sevi": 2},
    {"ar": "نَحْنُ نَذْهَبُ إِلَى الْمَتْجَر", "tr": "Biz mağazaya gidiyoruz.", "sevi": 2},
    {"ar": "الطَّعَامُ لَذِيذٌ جِدًا", "tr": "Yemek çok lezzetli.", "sevi": 2},
    {"ar": "لَا أَتَكَلَّمُ الْعَرَبِيَّة جَيِّدًا", "tr": "Arapça'yı iyi konuşamıyorum.", "sevi": 2},
    {"ar": "أَيْنَ كِتَابِي؟", "tr": "Kitabım nerede?", "sevi": 2},
    {"ar": "أَنَا أُحِبُّ عَمَلِي", "tr": "İşimi seviyorum.", "sevi": 3},
    {"ar": "هِيَ تَشْرَبُ الْمَاء", "tr": "O su içiyor.", "sevi": 3},
    {"ar": "أَخِي طَوِيل وَأَنَا قَصِير", "tr": "Erkek kardeşim uzun, ben ise kısayım.", "sevi": 3},
    {"ar": "هَلْ قَرَأْتَ هَذَا الْكِتَاب؟", "tr": "Bu kitabı okudun mu?", "sevi": 3},
    {"ar": "هَذَا الْبَيْتُ قَدِيم", "tr": "Bu ev eskidir.", "sevi": 3},
    {"ar": "لِمَاذَا جِئْتَ مُتَأَخِّرًا؟", "tr": "Neden geç geldin?", "sevi": 4},
    {"ar": "سَنَذْهَبُ إِلَى الْمَطْعَم غَدًا", "tr": "Yarın restorana gideceğiz.", "sevi": 4},
    {"ar": "أَنَا سَعِيد بِرُؤْيَتِكَ", "tr": "Seni gördüğüme sevindim.", "sevi": 4},
    {"ar": "مَا هِيَ مُشْكِلَتُك؟", "tr": "Sorunun ne?", "sevi": 4},
    {"ar": "يُمْكِنُنِي مُسَاعَدَتُك", "tr": "Sana yardım edebilirim.", "sevi": 4},
    {"ar": "نَحْنُ نَنْتَظِرُ الْقِطَار", "tr": "Biz treni bekliyoruz.", "sevi": 5},
    {"ar": "الطَّقْسُ جَمِيل الْيَوْم", "tr": "Hava bugün güzel.", "sevi": 5},
    {"ar": "هَلْ تَسْتَطِيعُ الْقِيَادَة؟", "tr": "Araba kullanabilir misin?", "sevi": 5},
    {"ar": "أَنَا أُفَكِّرُ فِي الْمُسْتَقْبَل", "tr": "Geleceği düşünüyorum.", "sevi": 5},
    {"ar": "هَلْ هَذَا صَحِيح أَمْ خَطَأ؟", "tr": "Bu doğru mu yoksa yanlış mı?", "sevi": 5},
    {"ar": "سَأَلْتُهُ سُؤَالاً وَأَجَابَ", "tr": "Ona bir soru sordum ve cevapladı.", "sevi": 6},
    {"ar": "هَذَا الْمُهَنْدِس نَشِيط جِدًا", "tr": "Bu mühendis çok enerjik.", "sevi": 6},
    {"ar": "أُرِيدُ أَنْ أَشْتَرِي هَذَا الْقَلَم", "tr": "Bu kalemi satın almak istiyorum.", "sevi": 6},
    {"ar": "عُدْنَا إِلَى الْبَيْت فِي اللَّيْل", "tr": "Gece eve geri döndük.", "sevi": 6},
    {"ar": "أَنَا أَكْرَهُ الطَّعَام الْبَارِد", "tr": "Soğuk yemekten nefret ediyorum.", "sevi": 6},
    {"ar": "هِيَ دَائِمًا تَبِيعُ أَشْيَاء قَدِيمَة", "tr": "O her zaman eski şeyler satar.", "sevi": 7},
    {"ar": "هَذَا الشَّارِع طَوِيل وَبَطِيء", "tr": "Bu cadde uzun ve yavaş.", "sevi": 7},
    {"ar": "أَيْنَ سَتَذْهَبُ بَعْدَ الْغَدَاء؟", "tr": "Öğle yemeğinden sonra nereye gideceksin?", "sevi": 7},
    {"ar": "أَلْوَانُ هَذِهِ الصُّورَة جَمِيلَة", "tr": "Bu resmin renkleri güzel.", "sevi": 7},
    {"ar": "أَنَا لَسْتُ مُتَأَكِّدًا", "tr": "Emin değilim.", "sevi": 7},
    {"ar": "يَجِبُ أَنْ نَنْتَظِرَ حَتَّى الصَّبَاح", "tr": "Sabaha kadar beklemeliyiz.", "sevi": 8},
